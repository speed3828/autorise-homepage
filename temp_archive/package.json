{
  "name": "autorise-new",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3100",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "preview": "vite preview --port 3100",
    "predeploy": "npm run build && node scripts/archive.js",
    "deploy": "scp -r . $DEPLOY_USER@$DEPLOY_HOST:/var/www/myapp",
    "postdeploy": "ssh $DEPLOY_USER@$DEPLOY_HOST 'pm2 restart myapp'",
    "start:cursor": "kill-port 3100 && next dev -p 3100",
    "doctor": "node scripts/doctor.js",
    "export": "next build && next export",
    "static": "npm run build && npx serve out -p 3100",
    "static-server": "node scripts/static-server.js"
  },
  "dependencies": {
    "@emailjs/browser": "^4.1.0",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "open-cli": "^8.0.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "wait-on": "^8.0.3"
  }
}
